<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CollapsingBarLayout"/>

    <declare-styleable name="OpusILCollapsingBarLayout_Layout">
        <attr name="layout_orderFlag">
            <flag name="first" value="0x1"/>
            <flag name="second" value="0x2"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="GooeyFloatingActionButton">
        <attr name="parentButtonRadius" format="reference|dimension"/>
        <attr name="parentDrawable" format="reference"/>
        <attr name="childButtonRadius" format="reference|dimension"/>
        <attr name="childDrawable" format="reference"/>
        <attr name="childMenu" format="reference"/>
        <attr name="offset" format="reference|dimension"/>
    </declare-styleable>
</resources>